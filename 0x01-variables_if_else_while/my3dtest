#include <stdio.h>
/**
 * main - Entry point
 *
 * Return: Always 0 (success)
 */

int main(void)
{
	int a = 0;

	int b = 1;

	int c = 2;

	while
		(a < 10, b < 10, c < 10) {
			if (a != b && b != c && a != c)
			{
				putchar(a + '0');
				putchar(b + '0');
				putchar(c + '0');
			}
			if (a < 7)
			{
				putchar(',');
				putchar(' ');
			}
			a++;
			b = a + 1;
			c = b + 1;
		}
	putchar('\n');
	return (0);
}
